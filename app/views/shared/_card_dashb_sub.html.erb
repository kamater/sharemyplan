<div class="co-abo-card m-auto">
  <%= image_tag(subscription.service.photo, class: "co-abo-card-photo") %>
  <div class="co-abo-price"><%= subscription.service.total_price.fdiv(subscription.service.number_of_places).round(0) %>€ / mois</div>
  <div class="commission-service">+0,30 € de frais de service par mois</div>
  <%= link_to "Gérer", subscription_path(subscription), class: "btn btn-lg btn-dasb mt-4"  %>
</div>
